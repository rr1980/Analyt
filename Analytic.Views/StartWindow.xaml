<Window x:Class="Analytic.Views.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" 
        xmlns:local="clr-namespace:Analytic.Views"
        mc:Ignorable="d"
        Title="StartWindow" Height="400" Width="600" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <WrapPanel Grid.Row="0" Background="#FF999999">
            <Button Name="GoHomeBtn" Width="70" Height="20" Command="{Binding Path=GoHomePageCommand}">Home</Button>
            <Button Name="GoCpuBtn" Width="70" Height="20" Command="{Binding Path=GoCpuPageCommand}">Cpu</Button>
            <Button Name="TestBtn" Width="70" Height="20" Command="{Binding Path=TestCommand}">Test</Button>
            <ComboBox  Width="70" ItemsSource="{Binding History}" SelectedItem="{Binding HistorySelectedPage}"/>
        </WrapPanel>
        <DockPanel Grid.Row="1">
            <Frame x:Name="_mainFrame" NavigationUIVisibility="Hidden" />
        </DockPanel>
    </Grid>
</Window>
